{
  "kind": "singleType",
  "collectionName": "proyectos",
  "info": {
    "singularName": "proyecto",
    "pluralName": "proyectos",
    "displayName": "Proyecto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": { "type": "string" },
    "subtitulo": { "type": "string" },
    "anio": { "type": "integer" },
    "objetivo": { "type": "text" },
    "problema": { "type": "text" },
    "proceso": { "type": "text" },
    "tecnologias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tecnologia.tecnologia"
    },
    "equipo": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team-member.team-member"
    },
    "portada": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images"],
      "required": false
    },
    "secciones": {
      "type": "dynamiczone",
      "components": [
        "proyecto.secciones-del-proyecto",
        "proyecto.tecnologias",
        "proyecto.imagenes",
        "proyecto.equipo",
        "proyecto.contenido-general"
      ]
    }
  }
}
